{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
    "no-console": "off",
    "prefer-const": "off",
    "camelcase": "off",
    "no-underscore-dangle": "off",
    "consistent-return": "off",
    "object-shorthand": "off",
    "func-names": "off",
    "no-await-in-loop": "off"
  }
}
